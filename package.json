    "dayjs": "^1.11.13",
    "dhc-utils": "git+https://github.com/Dh-Center-Tuebingen/dhc-utils.git",
    "emoji-mart-vue-fast": "^15.0.4",
    "highlight.js": "^11.11.1",
    "vee-validate": "^4.13.2",
    "vue": "^3.3.0",
    "vue-i18n": "^9.13.1",
    "yup": "^1.6.1"
